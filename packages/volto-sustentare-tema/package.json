  "scripts": {
  "i18n": "rm -rf build/messages && NODE_ENV=production i18n --addon",
  "dry-release": "release-it --dry-run",
  "release": "release-it",
  "release-major-alpha": "release-it major --preRelease=alpha",
  "release-alpha": "release-it --preRelease=alpha",
  "storybook": "sh -c 'pnpm exec -- storybook dev -c ./.storybook -p 6007 || npx --no-install storybook dev -c ./.storybook -p 6007'",
  "build-storybook": "sh -c 'VOLTOCONFIG=$(pwd)/volto.config.js pnpm exec -- storybook build -c ./.storybook -o ./storybook-static || VOLTOCONFIG=$(pwd)/volto.config.js npx --no-install storybook build -c ./.storybook -o ./storybook-static'"
  },
  "addons": [],
